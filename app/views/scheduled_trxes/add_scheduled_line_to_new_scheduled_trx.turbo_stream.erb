<%= turbo_stream.append "trx-lines" do %>
  <%= fields_for @scheduled_trx do |f| %>
    <%= f.fields_for :scheduled_lines, @scheduled_line, child_index: Time.current.to_i do |scheduled_line_form| %>
      <%= render 'scheduled_line_fields', f: scheduled_line_form %>
    <% end %>
  <% end %>
<% end %> 